{
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,

    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "@ant-design/pro-chat": ["./src"],
      "@ant-design/pro-chat/*": ["src/*"]
    }
  }
}
